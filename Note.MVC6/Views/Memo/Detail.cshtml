
<div class="row">
    <div class="col-lg-12">
        <h3 class="page-header">상세 읽기 - @Model.MemoTitle</h3>
    </div>
</div>

<div class="card">
    <div class="card-header">
        <label>제목 : </label> @Model.MemoTitle
    </div>
    <div class="card-body">
        <div class="card-text">
            @Html.Raw(Model.MemoContents)
        </div>
    </div>
    <div class="card-footer text-right">
        <a class="btn btn-primary" asp-controller="Memo" asp-action="Index">목록</a>
    </div>

</div>